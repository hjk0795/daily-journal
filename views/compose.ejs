<h1>Compose</h1>
<form action="/compose" method="post">
  <label for="postTitle" class="for-label">Title</label><br />
  <input type="text" name="postTitle" class="for-control" /><br />
  <label for="postBody" class="for-label">Post</label><br />
  <textarea
    name="postBody"
    cols="30"
    rows="10"
    class="for-control"
  ></textarea
  ><button type="submit" name="publish" value="publish" class="btn btn-primary">Publish</button>
</form>

